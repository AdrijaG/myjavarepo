package com.demo.bean;

import java.util.Date;

public class Employee extends Person{
     private String dept;
     private String desg;
     public Employee() {
    	 System.out.println("in employee default constructor");
    	 dept=null;
    	 desg=null;
     }
     
     public Employee(int id,String perName,Date bDate,String mob,String dept,String desg) {
    	 System.out.println("in employee parametrised constructor");
    	 this.dept=dept;
    	 this.desg=desg;
     }
     
     public String toString() {
    	 return super.toString()+"Department :"+dept+"\nDesignation  : "+desg;
    	 
     }
	
}
